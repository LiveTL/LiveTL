{
  "name": "LiveTL",
  "version": "6.7.0",
  "description": "Get live translations for YouTube streams, crowdsourced from multilingual viewers!",
  "scripts": {
    "build": "node utils/build.js",
    "package": "node utils/package.js",
    "start": "node utils/webserver.js",
    "ss": "node utils/screenshot.js",
    "update:list": "node utils/genlist.js",
    "type:check": "tsc -p jsconfig.json",
    "test": "sed -i 's/import noUi/import * as noUi/g' node_modules/svelte-materialify/src/components/Slider/Slider.svelte && jest src",
    "test:watch": "yarn test --watch",
    "e2e:build": "yarn build production && yarn package && cp dist/LiveTL-Firefox.zip dist/LiveTL-Firefox.xpi",
    "e2e:test": "pytest --tests-per-worker auto e2e",
    "e2e": "yarn e2e:build && yarn e2e:test",
    "format": "eslint --fix .",
    "format:check": "eslint --fix-dry-run"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^13.0.16",
    "babel-jest": "^26.6.3",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "5.2.6",
    "deepmerge": "^4.2.2",
    "eslint": "^7.22.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "file-loader": "6.2.0",
    "filemanager-webpack-plugin": "^6.1.4",
    "fs-extra": "7.0.1",
    "glob-parent": "^6.0.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "jest": "^26.6.3",
    "postcss": "^8.3.5",
    "sass": "^1.32.8",
    "sass-loader": "^12.1.0",
    "sockjs": "^0.3.21",
    "string-replace-loader": "^3.0.2",
    "style-loader": "^2.0.0",
    "svelte": "^3.5.1",
    "svelte-jester": "^1.3.1",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.6.9",
    "typescript": "^4.3.5",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.39.1",
    "webpack-concat-files-plugin": "^0.5.2",
    "webpack-dev-server": "3.11.2",
    "ws": "^7.5.0",
    "xvfb": "^0.4.0"
  },
  "dependencies": {
    "@mdi/js": "^5.9.55",
    "cache-loader": "^4.1.0",
    "dns-packet": "^5.2.4",
    "file-saver": "^2.0.5",
    "hosted-git-info": "^4.0.2",
    "html2canvas": "^1.0.0-rc.7",
    "jquery": "^3.6.0",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "reconnecting-eventsource": "^1.1.0",
    "ssri": "^8.0.1",
    "svelte-materialify": "0.3.6",
    "vue": "^2.6.12",
    "vuetify": "^2.4.9",
    "youtube-iframe": "^1.0.3"
  }
}
